Board	spec/board_spec.rb	/^class Board$/;"	c
BoardHelpers	spec/support/board_helpers.rb	/^module BoardHelpers$/;"	m
Game	spec/game_spec.rb	/^class Game$/;"	c
Player	spec/player_spec.rb	/^class Player$/;"	c
check_winner	spec/game_spec.rb	/^  def check_winner$/;"	f	class:Game
disc_check	spec/player_spec.rb	/^  def disc_check(disc_to_check)$/;"	f	class:Player
error_check	spec/board_spec.rb	/^  def error_check(pos)$/;"	f
fill_up_board	spec/support/board_helpers.rb	/^  def fill_up_board(board)$/;"	f	class:BoardHelpers
full?	spec/board_spec.rb	/^  def full?$/;"	f
initialize	spec/board_spec.rb	/^  def initialize$/;"	f	class:Board
initialize	spec/game_spec.rb	/^  def initialize(player1, player2, board)$/;"	f	class:Game
initialize	spec/player_spec.rb	/^  def initialize(name, disc)$/;"	f	class:Player
insert	spec/board_spec.rb	/^  def insert(pos, disc)$/;"	f	class:Board
